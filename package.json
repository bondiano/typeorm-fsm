{
  "name": "typeorm-fsm",
  "version": "0.0.1",
  "author": "Vassiliy Kuzenkov (bondiano)",
  "license": "MIT",
  "description": "Strong typed state machine for your TypeORM Entities",
  "repository": {
    "type": "git",
    "url": "https://github.com/bondiano/typeorm-fsm"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "typeorm",
    "fsm",
    "state-machine"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "commit": "cz",
    "doc": "typedoc src/index.ts --tsconfig tsconfig.build.json --plugin typedoc-plugin-mermaid"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/better-sqlite3": "^7.6.4",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "better-sqlite3": "^8.5.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "jest": "^29.6.4",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.0",
    "typedoc-plugin-mermaid": "^1.10.0"
  },
  "peerDependencies": {
    "typeorm": "~0.3.0"
  },
  "commit": "cz",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
